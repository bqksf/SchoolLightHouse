.term-weeks {
  text-align: center;
}

.toolbox-button{
  display: flex;
  justify-content: center;
}


.head-title{
  margin: 10rpx;
  padding: 0 10px;
  color: #666;
  font-size: 30rpx;
  font-weight: 450;
}

.notice{
  margin: 10rpx;
  padding: 0 20rpx;
  color: #171a20;
  font-size: 26rpx;
}

.bigTitle{
  font-size: 60rpx;
  color: #000;
}

.notice-icon{
  padding-left: 30rpx;
  display: flex;
  align-items: center;
}

.icon-info{
  width: 35rpx;
  height: 35rpx;
  display: inline-block;
  background-size: cover;
  background-image:url("data:image/png;base64,");
}

.class{
  padding: 20rpx 0;
  text-align: center;
/*   height: 3.3em;
  line-height: 3.3em; */
  font-size: 28rpx;
  color: #171a20;
}

.no-class {
  margin: 10rpx;
  padding: 0 20rpx;
  text-align: center;
  height: 3.3em;
  line-height: 3.3em;
  font-size: 28rpx;
  color: #666;
}

.menu{
  margin: 10rpx;
  padding: 20rpx;
  background-color: #eee;
  color: #171a20;
  border-radius: 15rpx;
  font-size: 35rpx;
  font-weight: 700;
}

.menu-imgs{
  display: flex;
  margin-bottom: 30rpx;
}

.menu-tip{
  margin-top: -8rpx;
  font-size: 20rpx;
  color: #666;
}

.menu-img{
  width: 50rpx;
  height: 50rpx;
}

.pg-menus{
  margin: 0 20rpx;
}

button:not([size='mini']){
  width: 100% !important;
  margin: 15px !important;
  align-items: center;
  justify-content: center;
  display: flex;
}

.toolbox-title{
  margin-top: 20rpx;
  padding: 0 5px;
  font-size: 50rpx;
  color: #000;
  font-weight: 450;
}

.menu-hover{
  background-color: #d0d1d2;
}

.tool-img {
  width: 35rpx;
  height: 35rpx;
  padding-right: 10rpx;
}  

.weui-cells__title {
  font-size: 26rpx;
  color: #666;
  padding-top: 10rpx;
  font-weight: 200;
  margin-bottom: 0px;
  text-align: right;
}