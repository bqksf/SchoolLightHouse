<view class="page">
  <view class="page__hd">
    <view class="page__title">Uploader</view>
    <view class="page__desc">上传组件</view>
  </view>
  <view class="page__bd">
    <mp-cells>
      <mp-cell>
        <mp-uploader title="图片上传" tips="图片上传提示" bindfail="uploadError" bindsuccess="uploadSuccess" binddelete="uploaddelete"
         upload="{{uplaodFile}}" files="{{files}}" max-count="5"></mp-uploader>
      </mp-cell>
    </mp-cells>
  </view>
</view>